{
  "name": "@inkofpixel/liquid-loader",
  "version": "0.0.5",
  "description": "Webpack loader for custom liquid files",
  "main": "dist/cjs.js",
  "repository": "https://github.com/inkOfPixel/liquid-loader",
  "author": "@forgng",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "scripts": {
    "start": "yarn build -- -w",
    "clean": "del-cli dist",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "commitlint": "commitlint --from=main",
    "security": "npm audit --production",
    "lint:prettier": "prettier --list-different .",
    "lint:js": "eslint --cache .",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "prepare": "husky install && yarn build",
    "test": "jest"
  },
  "dependencies": {
    "babel-jest": "27.0.2",
    "chalk": "4.1.1",
    "jest": "27.0.4",
    "to-string-loader": "1.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@inkofpixel/liquid-loader": "0.0.2",
    "autoprefixer": "10.2.6",
    "babel-loader": "8.2.2",
    "chalk": "4.1.1",
    "commander": "7.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "5.2.6",
    "cssnano": "5.0.6",
    "del": "^6.0.0",
    "del-cli": "^4.0.0",
    "extract-loader": "5.1.0",
    "husky": "6.0.0",
    "ink": "3.0.8",
    "json-loader": "0.5.7",
    "lint-staged": "^11.0.0",
    "node-emoji": "1.10.0",
    "pastel": "^1.1.1",
    "postcss": "8.3.5",
    "postcss-loader": "6.1.0",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2",
    "raw-loader": "4.0.2",
    "sass": "1.35.1",
    "sass-loader": "12.1.0",
    "style-loader": "2.0.0",
    "style-resources-loader": "1.4.1",
    "ts-loader": "9.2.3",
    "typescript": "4.3.4",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "5.39.1",
    "webpack-build-notifier": "2.2.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0",
    "webpack-sources": "2.3.0"
  }
}
